  "version": "0.2.0",
  "configurations": [
    {
      "name": "Revalexo",
      "type": "debugpy",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "program": "main.py",
      "args": [
        "-e", "project=Revalexo", "type=Transparent", "trial=0",
        "-f", "laptop.yml",
        "-o", "data"
      ],
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "console": "integratedTerminal",
      "subProcess": true,
      "justMyCode": false
    },
  ]
}
